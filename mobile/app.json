      "apiBaseUrl": "http://192.168.1.165:3000"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share product images.",
          "cameraPermission": "The app accesses your camera to let you take product photos."
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "primaryColor": "#F59E0B"
